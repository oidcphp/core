{
    "name": "oidc/core",
    "description": "OpenID Connect Core Library implementation for PHP",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "oidc",
        "openid connect"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/oidcphp/core/issues"
    },
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "mileschou/psr": "^2.0",
        "paragonie/constant_time_encoding": "^2.6 | ^3",
        "psr/clock": "^1.0",
        "psr/container": "^1.0 | ^2.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0 | ^2.0",
        "web-token/jwt-library": "^4.0"
    },
    "suggest": {
        "laminas/laminas-diactoros": "Require if use Laravel and Laminas diactoros"
    },
    "require-dev": {
        "illuminate/container": "^6 | ^7 | ^8 | ^9 | ^10",
        "illuminate/support": "^6 | ^7 | ^8 | ^9 | ^10",
        "laminas/laminas-diactoros": "^3",
        "phpunit/phpunit": "^9.6",
        "slevomat/coding-standard": "^8.19",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/var-dumper": "^5.4 | ^6",
        "symfony/yaml": "^5.4 | ^6"
    },
    "autoload": {
        "psr-4": {
            "OpenIDConnect\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "platform": {
            "ext-sodium": "1"
        },
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
